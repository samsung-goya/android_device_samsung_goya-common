<?xml version="1.0" encoding="utf-8"?>
<MarvellSensorConfiguration>
  <SensorSet identifier="default">
    <Sensor identifier="Pressure">
      <lib name="libsensors.st.pressure.so"/>
      <name value="ST Pressure sensor"/>
      <vendor value="STMicroelectronics"/>
      <version value="1"/>
      <!-- handle is identical with type -->
      <handle value="SENSOR_TYPE_PRESSURE"/>
      <type value="SENSOR_TYPE_PRESSURE"/>
      <maxRange value="5000"/>
      <resolution value="10"/>
      <power value="60"/>
      <minDelay value="10"/>
    </Sensor>
    <Sensor identifier="Temperature">
      <lib name="libsensors.st.pressure.so"/>
      <name value="ST Temperature sensor"/>
      <vendor value="STMicroelectronics"/>
      <version value="1"/>
      <!-- handle is identical with type -->
      <handle value="SENSOR_TYPE_AMBIENT_TEMPERATURE"/>
      <type value="SENSOR_TYPE_AMBIENT_TEMPERATURE"/>
      <maxRange value="5000"/>
      <resolution value="10"/>
      <power value="60"/>
      <minDelay value="10"/>
    </Sensor>
  </SensorSet>
</MarvellSensorConfiguration>